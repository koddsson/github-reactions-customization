{
  "manifest_version": 2,
  "name": "GitHub Reaction Customizer",
  "version": "1.0",
  "browser_action": {
    "default_title": "Github Reaction Customizer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}